<select id='routine-select' onchange={{action selectRoutine value="target.value"}}>
  {{#each model.routines.routines as |routine|}}
    <option value={{routine.name}} selected={{eq routine.name routineChoice}}>{{routine.name}}</option>
  {{/each}}
</select>

<button {{action 'modelTeller'}}>Model Teller</button>

